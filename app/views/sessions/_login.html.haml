#login
- if @current_user
  %p.welcome Welcome, #{@current_user.provider.capitalize} user #{@current_user.name}!
  = link_to 'Log Out', logout_path
- else
  -#%p.login= link_to 'Log in with Twitter', '/auth/twitter'
  %p.login
  Log in with
  - %w(Twitter Facebook).each do |media|
    = link_to media, login_path(:provider => media)
