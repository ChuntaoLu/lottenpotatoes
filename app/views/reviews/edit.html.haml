%h2= "Edit My Review for #{@movie.title}"
= form_tag movie_review_path(@movie, @review), :method => :put do
  = label :review, :potatoes, "Potatoes"
  = select_tag 'review[potatoes]', options_for_select(1..5)

  = label :review, :commnets, "Comments"
  = text_field :review, 'comments'

  = submit_tag 'Update Review'
